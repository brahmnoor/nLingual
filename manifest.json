{
  "manifest_version": 2,
  "name": "nLingual",
  "description": "learn n (where n > 1) languages easily.",
  "version": "0.1",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["packages/jquery.min.js", "packages/bootstrap.min.js", "content/replaceWords.js"],
      "run_at": "document_end"
    }
  ]
}
